{
    "multipart": [
        {   "apply": { "model": "spruce_leaves" }},
        
        {   "when": { "OR": [{ "north": "false", "variant": "type0" }, { "north": "false", "variant": "type1" }]},
            "apply": { "model": "floricraft:ornaments_ball_twinkling01" }
        },
        {   "when": { "OR": [{ "east": "false", "variant": "type0" }, { "east": "false", "variant": "type1" }]},
            "apply": { "model": "floricraft:ornaments_ball_twinkling01", "y": 90 }
        },
        {   "when": { "OR": [{ "south": "false", "variant": "type0" }, { "south": "false", "variant": "type1" }]},
            "apply": { "model": "floricraft:ornaments_ball_twinkling01", "y": 180 }
        },
        {   "when": { "OR": [{ "west": "false", "variant": "type0" }, { "west": "false", "variant": "type1" }]},
            "apply": { "model": "floricraft:ornaments_ball_twinkling01", "y": 270 }
        },
        {   "when": { "OR": [{ "up": "false", "variant": "type0" }, { "up": "false", "variant": "type1" }]},
            "apply": { "model": "floricraft:ornaments_ball_twinkling01", "x": 270 }
        },
        
        {   "when": { "north": "false", "variant": "type1" },
            "apply": { "model": "floricraft:ornaments_ribbon01" }
        },
        {   "when": { "east": "false", "variant": "type1" },
            "apply": { "model": "floricraft:ornaments_ribbon01", "y": 90 }
        },
        {   "when": { "south": "false", "variant": "type1" },
            "apply": { "model": "floricraft:ornaments_ribbon01", "y": 180 }
        },
        {   "when": { "west": "false", "variant": "type1" },
            "apply": { "model": "floricraft:ornaments_ribbon01", "y": 270 }
        }
    ]
}
